{
  "name": "uix-starter",
  "scripts": {
    "dev": "npx shadow-cljs -A:dev watch app & npm run tailwind-dev & npm run shadcn-watch",
    "tailwind-dev": "npx @tailwindcss/cli -i ./src/css/main.css -o ./public/main.css --watch",
    "shadcn-watch": "npx tsc --build --force --watch & npx tsc-alias --watch",
    "build": "npm run build:shadcn && npm run build:css && npm run build:cljs",
    "build:dev": "npm run build:shadcn && npm run build:css && npm run build:cljs:dev",
    "build:staging": "npm run build:shadcn && npm run build:css && npm run build:cljs:staging",
    "build:prod": "npm run build:shadcn && npm run build:css && npm run build:cljs:prod",
    "build:shadcn": "npx tsc --build && npx tsc-alias",
    "build:css": "npx @tailwindcss/cli -i ./src/css/main.css -o ./public/main.css --minify",
    "build:cljs": "npx shadow-cljs -A:dev release app",
    "build:cljs:dev": "npx shadow-cljs -A:dev release app --config-merge '{:closure-defines {config.core/API_BASE_URL \"http://localhost:8081\"}}'",
    "build:cljs:staging": "npx shadow-cljs -A:dev release app --config-merge '{:closure-defines {config.core/API_BASE_URL \"https://staging-api.bryc.obney.cloud\"}}'",
    "build:cljs:prod": "npx shadow-cljs -A:dev release app --config-merge '{:closure-defines {config.core/API_BASE_URL \"https://api.bryc.obney.cloud\"}}'",
    "clean": "npx shadow-cljs -A:dev clean"
  },
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-refresh": "^0.17.0",
    "shadcn": "^2.10.0",
    "shadow-cljs": "3.2.0",
    "tailwindcss": "^4.1.12",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@js-joda/core": "3.2.0",
    "@js-joda/locale_en-us": "3.1.1",
    "@js-joda/timezone": "2.5.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
